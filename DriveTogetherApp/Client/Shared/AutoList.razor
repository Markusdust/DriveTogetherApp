@inject IAutoService AutoService

@if(AutoService.Autos == null || AutoService.Autos.Count == 0)
{
    <span>Loading Autos...</span>
}else
{

@using DriveTogetherApp.Shared.Model
<ul class="list-unstyled">
        @foreach (var auto in AutoService.Autos)
        {
        <div class="medai-body">
            <a href="#">
                <h4 class="mb-0">@auto.Fahrtenanbieter</h4>
            </a>
            <p>@auto.Marke</p>
            <h5 class="price">
                @auto.Modell
            </h5>
        </div>
    }
</ul>
}
@code {
    protected override async Task OnInitializedAsync()
    {
        await AutoService.GetAutos();
    }
}

 